<li style='position: relative;' data-toggle="tooltip" title="<%= single_file.file_display_name %> (<%= single_file.duration.present? ? Time.at(single_file.duration).utc.strftime('%H:%M:%S') : '00:00:00' %>)" data-type="index" class="listings_files <%= can_access ? 'list_can_access' : '' %> <%= single_file.id == @resource_file.id ? ' my-slide ' : '' %>" title="<%= single_file.file_display_name && !single_file.file_display_name.empty? ? single_file.file_display_name : single_file.resource_file_file_name && !single_file.resource_file_file_name.empty? ? single_file.resource_file_file_name : 'Filename ' + index.to_s %>" >