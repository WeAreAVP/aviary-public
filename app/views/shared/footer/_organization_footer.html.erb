<footer id="footer" class="footer_section">
  <div class="container">
    <div class="row">
      <div class="col-lg-8">
        <div class="row">
          <div class="col-md-4">
            <div class="logo" id="logoCst">
              <h3><%== footer_title %></p></h3>
            </div>
          </div>
          <div class="col-md-8">
            <ul id="footer_menu">
              <%= footer_menu %>
            </ul>
          </div>
        </div>
      </div>
      <div class="col-lg-4">
        <div class="row">
          <div class="col-md-5">
            <p class="address"><%= footer_address %></p>
          </div>
          <div class="col-md-3">
            <p class="hours"><%= footer_hours %></p>
          </div>
          <div class="col-md-4">
            <p class="copyright">
              <%= footer_copy_rights %></p>
          </div>
        </div>
      </div>
    </div>
  </div>
</footer>
