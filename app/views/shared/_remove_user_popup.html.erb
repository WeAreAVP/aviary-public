<div class="modal fade" id="remove_organization_user" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-md modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Remove User</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body" >
        <p>Are you sure you want to remove this user?</p>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" id="class_message" data-dismiss="modal">Close</button>
        <%= link_to "Remove", "", class: "btn btn-danger remove-organization-user-btn" %>
      </div>
    </div>
  </div>
</div>
