<div class="modal fade" id="transcript_upload" tabindex="-1" role="dialog" aria-labelledby="transcriptUploadLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="transcript_upload_title">Transcript Upload</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <%= render 'transcripts/form', file_trancript: @file_trancript, is_new: true %>
      </div>
    </div>
  </div>
</div>