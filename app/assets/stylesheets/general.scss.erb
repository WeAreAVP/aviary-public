.custom-checkbox label {
  display: inline-block;
  vertical-align: middle;
  padding: 1px 0 0px 25px;
  position: relative;
  font-size: 14px;
  font-weight: 500;
  margin: 0;
  color: #000B11;
  cursor: pointer;
}

.custom-checkbox label:before {
  width: 17px;
  height: 17px;
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  background: #fff;
  border: 2px solid #336075;
  border-radius: 2px;
}

.custom-checkbox label:after {
  width: 9px;
  height: 5px;
  content: '';
  border-width: 0 0 2px 2px;
  border-style: solid;
  border-color: #336074;
  position: absolute;
  top: 5px;
  left: 4px;
  -webkit-transform: rotate(-45deg);
  transform: rotate(-45deg);
  display: none;
}

.custom-checkbox input[type=radio].resolve:checked + label {
  color: var(--success);
}

.custom-checkbox input[type=radio].unresolve:checked + label {
  color: var(--danger);
}

.custom-checkbox input[type=radio].resolve:checked + label:before,
.custom-checkbox input[type=radio].resolve:checked + label:after {
  border-color: var(--success);
}

.custom-checkbox input[type=radio].unresolve:checked + label:before,
.custom-checkbox input[type=radio].unresolve:checked + label:after {
  border-color: var(--danger);
}

.custom-checkbox input[type=radio]:checked + label:after,
.custom-checkbox input[type=checkbox]:checked + label:after {
  display: block;
}

