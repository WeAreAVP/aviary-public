.alert {
  position: fixed;
  padding: 15px 50px 15px 40px;
  margin-bottom: 1rem;
  top: 59px;
  transition: 1.0s all ease-in-out;
  right: 15px;
  border-radius: 8px;
  font-weight: 400;
  overflow: hidden;
  border: none;
  border: 1px solid;
  z-index: 999999 !important;
}

.alert:before {
  display: none;
}

.alert .close {
  position: absolute;
  top: 11px;
  right: 15px;
  font-weight: 700;
  opacity: 0.5;
  font-size: 16px;
  line-height: 24px;
  width: 20px;
  border-radius: 20px;
  height: 20px;
}

.alert .close:after {
  content: "";
  position: absolute;
  top: 2px;
  right: 0;
  width: 20px;
  height: 20px;
  border: 1px solid #0e0e0e;
  border-radius: 20px;
}

.alert .close:hover {
  opacity: 0.7;
}

.alert.alert-success {
  background-color: #d3ffda;
  border-top-color: #34c15d !important;
  color: #1fa947;
  line-height: 1;
  text-align: center;
}

.alert.alert-success .close,
.alert.alert-success .close:after {
  color: #1fa947;
  border-color: #1fa947;
}

.alert.alert-success:after {
  content: "";
  position: absolute;
  top: 15px;
  left: 15px;
  width: 15px;
  height: 15px;
  background: image-url(img/check) no-repeat;
  background-size: 15px;
}

.alert.alert-danger {
  background-color: #ffdede;
  color: #cc191a;
  line-height: 1;
  text-align: left;
}

.alert.alert-danger .close,
.alert.alert-danger .close:after {
  color: #cc191a;
  border-color: #cc191a;
}

.alert.alert-danger:after {
  content: "";
  position: absolute;
  top: 15px;
  left: 15px;
  width: 15px;
  height: 15px;
  background: image-url(img/delete) no-repeat;
  background-size: 15px;
}

.alert.alert-warning {
  background-color: #f5e8c2;
  color: #b3880d;
  line-height: 1;
  text-align: left;
}

.alert.alert-warning .close,
.alert.alert-warning.alert.alert-success:after.alert.alert-success:after .close:after {
  color: #b3880d;
  border-color: #b3880d;
}

